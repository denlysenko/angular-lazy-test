<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ua.qoderoom.user.model">
    <class name="ua.qoderoom.user.model.User" table='"USER"'>
        <id name="id" type="java.lang.String" column='"ID"'/>
        <property name="login" type="java.lang.String" column='"LOGIN"'/>
        <property name="createDate" type="java.util.Calendar" column='"CREATE_DATE"'/>
    </class>
</hibernate-mapping>